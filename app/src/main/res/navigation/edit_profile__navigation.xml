<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/edit_profile__navigation"
    app:startDestination="@id/editProfileFragment">

    <fragment
        android:id="@+id/editProfileFragment"
        android:name="com.g3.spot_guide.screens.profile.editProfile.EditProfileFragment"
        android:label="EditProfileFragment"
        tools:layout="@layout/edit_profile__fragment">
        <action
            android:id="@+id/action__editProfileFragment_to_galleryFragment"
            app:destination="@id/galleryFragment" />
    </fragment>

    <fragment
        android:id="@+id/galleryFragment"
        android:name="com.g3.spot_guide.screens.gallery.GalleryFragment"
        android:label="GalleryFragment"
        tools:layout="@layout/gallery__fragment" />

</navigation>