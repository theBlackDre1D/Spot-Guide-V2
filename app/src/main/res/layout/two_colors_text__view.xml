<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/grayTV"
        style="@style/normal_sfpro_text__16__grey_272637"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="Don't have an account?" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/purpleTV"
        style="@style/normal_sfpro_text__16__purple_6B14A6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="6dp"
        app:layout_constraintStart_toEndOf="@id/grayTV"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="Register" />

</androidx.constraintlayout.widget.ConstraintLayout>